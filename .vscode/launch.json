// Runs the built .dll when debugging
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Api",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-api",
      "program": "${workspaceFolder}/src/WebApi/bin/Debug/net10.0/WebApi.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/WebApi",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    }
  ]
}
